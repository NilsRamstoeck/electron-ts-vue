{
   "name": "electron-ts-vue",
   "version": "0.0.1",
   "main": "src/dist/electron/main.js",
   "scripts": {
      "electron:dev": "concurrently -k -s first \"cross-env BROWSER=none npm run dev\" \"tsc -w -p tsconfig.electron.json\" \"npm run electron\"",
      "preelectron:dev": "tsc -p tsconfig.electron.json",
      "app:build": "npm run build:for:electron && npm run electron:builder",
      "dev": "vite",
      "build": "vue-tsc --noEmit && vite build",
      "serve": "vite preview",
      "electron": "wait-on tcp:3000 && cross-env IS_DEV=true electron .",
      "electron:pack": "electron-builder --dir",
      "electron:builder": "electron-builder",
      "build:for:electron": "vue-tsc --noEmit && cross-env ELECTRON=true vite build",
      "test": "cross-env IS_TEST=true ts-mocha -p test.tsconfig.json test/tests.ts --verbose"
   },
   "dependencies": {
      "vue": "^3.2.25"
   },
   "devDependencies": {
      "@types/chai": "^4.3.0",
      "@types/mocha": "^9.1.0",
      "@vitejs/plugin-vue": "^2.2.0",
      "chai": "^4.3.6",
      "concurrently": "^7.0.0",
      "cross-env": "^7.0.3",
      "electron": "^17.0.1",
      "electron-builder": "^22.14.13",
      "electron-reloader": "^1.2.3",
      "mocha": "^9.2.2",
      "ts-mocha": "^9.0.2",
      "typescript": "^4.5.4",
      "vite": "^2.8.4",
      "vue-tsc": "^0.29.8",
      "wait-on": "^6.0.1"
   },
   "author": "Nils Ramst√∂ck",
   "description": "A template for an Electron App with Vue 3 Typescript using Vite",
   "license": "MIT",
   "bugs": {
      "url": "https://github.com/NilsRamstoeck/electron-ts-vue/issues"
   },
   "homepage": "https://github.com/NilsRamstoeck/electron-ts-vue#readme"
}
