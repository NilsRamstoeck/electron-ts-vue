{
   "name": "electron-ts",
   "version": "0.0.1",
   "main": "dist/compiled/electron/main.js",
   "scripts": {
      "dev": "concurrently -k -s first \"npm run vite\" \"npm run tsc:watch:electron\" \"npm run electron\"",
      "predev": "npm run tsc:electron",
      "build": "npm run compile && npm run electron:builder",
      "vite": "vite",
      "tsc:electron": "tsc -p tsconfig.electron.json",
      "tsc:watch:electron": "tsc -w -p tsconfig.electron.json",
      "compile": "vite build && tsc -p tsconfig.electron.json",
      "electron": "wait-on tcp:3000 && cross-env IS_DEV=true electron .",
      "electron:builder": "electron-builder",
      "test": "cross-env IS_TEST=true ts-mocha -p test.tsconfig.json test/tests.ts --verbose"
   },
   "build": {
      "files": [
         "./dist/compiled/**/*"
       ]
   },
   "dependencies": {
   },
   "devDependencies": {
      "@types/chai": "^4.3.1",
      "@types/mocha": "^9.1.1",
      "@typescript-eslint/eslint-plugin": "^5.30.5",
      "electron-builder": "^23.1.0",
      "electron": "^19.0.9",
      "eslint": "^8.19.0",
      "chai": "^4.3.6",
      "concurrently": "^7.3.0",
      "cross-env": "^7.0.3",
      "mocha": "^9.2.2",
      "ts-mocha": "^9.0.2",
      "typescript": "^4.7.4",
      "vite": "^2.9.14",
      "wait-on": "^6.0.1"
   },
   "author": "Nils Ramst√∂ck",
   "description": "A template for an Electron App with Typescript using Vite",
   "license": "MIT",
   "bugs": {
      "url": "https://github.com/NilsRamstoeck/electron-ts-vue/issues"
   },
   "homepage": "https://github.com/NilsRamstoeck/electron-ts-vue#readme"
}
